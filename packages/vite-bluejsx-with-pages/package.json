{
  "name": "vite-with-blue-pages",
  "version": "1.0.8",
  "description": "use pages with bluejsx and vite",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build-swc": "swc src/*.ts -d lib -C jsc.target=es2021",
    "build": "tsup src/index.ts --format cjs,esm --dts --minify -d lib"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git://github.com/bluejsx/plugins.git"
  },
  "author": "Momijiichigo",
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "directory-tree": "latest",
    "vite-with-bluejsx": "latest"
  },
  "devDependencies": {
    "vite": "latest",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.2",
    "tsup": "^5.4.0",
    "typescript": "^4.4.4"
  }
}
